		</dependency>
		<dependency>
    		<groupId>com.atlassian.jira</groupId>
    		<artifactId>jira-rest-java-client-core</artifactId>
  	  		<version>2.0.0-m19</version>
		</dependency>
		<dependency>
			<groupId>org.mockito</groupId>
			<artifactId>mockito-all</artifactId>
			<version>1.8.4</version>
		</dependency>
		
	</dependencies>
</project>